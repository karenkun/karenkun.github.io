{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "post",
      "pageBundle": false,
      "previewPath": null,
      "fileType": "markdown",
      "filePrefix": "YYYY-MM-DD",
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Slug date",
          "name": "slugdate",
          "type": "string",
          "default": "{{year}}/{{month}}/{{day}}"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories (limit 1)",
          "name": "categories",
          "type": "categories",
          "default" : "general"
        },
        {
          "title": "Page Layout",
          "name": "layout",
          "type": "choice",
          "multiple": false,
          "choices": [
            "post",
            "page"
          ],
          "default": "post"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "string",
          "default": "post"
        },
        {
          "title": "Slug (matches filename)",
          "name": "slug",
          "type": "string",
          "default": "{{slug}}"
        }
      ]
    },
    {
      "name": "page",
      "pageBundle": false,
      "previewPath": null,
      "fileType": "markdown",
      "filePrefix": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "choice",
          "multiple": false,
          "choices": [
            "post",
            "page"
          ],
          "default": "page"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string",
          "default": "page"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string",
          "default": "{{slug}}"
        },
        {
          "title": "Order",
          "name": "order",
          "type": "number"
        },
        {
          "title": "permalink",
          "name": "permalink",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.content.pageFolders": [
    {
      "title": "post",
      "path": "[[workspace]]/_posts",
      "previewPath": "{{fm.categories}}/{{fm.slugdate}}",
      "contentTypes": [
        "post"
      ]
    }
  ],
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": [
    " ",
    "a",
    "d",
    "e",
    "general",
    "j",
    "jekyll",
    "k",
    "l",
    "p",
    "t",
    "u",
    "y"
  ]
}